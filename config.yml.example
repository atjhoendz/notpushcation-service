env: "development"
ports:
  http: "9090"
enable_introspection: "true"
onesignal:
  api_key: ""
  app_id: ""
cockroach:
  host: "localhost:26257"
  database: "dbname"
  username: "root"
  password: ""
  sslmode: "disable"
  max_idle_conns: 2
  conn_max_lifetime: 3600000
  ping_interval: 5000
nats_js:
  host: "nats://localhost:4222"
  retry_on_failed_connect: true
  max_reconnect: -1
  reconnect_wait: "1s"
  retry_attempts: 5
  retry_interval: "2s"
  subscribe_retry_attempts: 5
  subscribe_retry_interval: "2s"
  stream_max_age: "24h"
  stream_max_messages: "100000"
redis:
  dial_timeout: 5
  write_timeout: 2
  read_timeout: 2
  max_idle_conn: 20
  max_active_conn: 50
  cache_host: "redis://localhost:6379/0"
  rate_limiter_host: "redis://localhost:6379/1"
enable_rate_limiter: "true"
rate_limiter:
  period: "1s"
  request_limit: "20"
  excluded_ips:
    - 127.0.0.1
    - 127.0.0.2
